<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:car_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/cardinsertadmin"
    car_view:cardElevation="3dp"
    car_view:cardCornerRadius="5dp"
    car_view:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/namamhs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="Nama Mahasiswa" />

        <TextView
            android:id="@+id/nimmhsi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="NIM Mahasiswa"
            android:layout_marginLeft="10dp"/>

    </LinearLayout>
</androidx.cardview.widget.CardView>